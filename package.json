{
    "author": "Goran Gligorin <goran@gligorin.com> (https://www.gorangligorin.com)",
    "bin": {
        "gligoran": "./bin/card.js"
    },
    "dependencies": {
        "boxen": "^4.0.0",
        "chalk": "^4.1.0"
    },
    "description": "A personal card for Goran Gligorin (@gligoran)",
    "devDependencies": {
        "ava": "3.10.1",
        "codecov": "3.7.2",
        "eslint": "7.5.0",
        "eslint-config-airbnb-base": "14.2.0",
        "husky": "4.2.5",
        "nyc": "15.1.0",
        "semantic-release": "17.1.1",
        "sinon": "9.0.2",
        "standard": "14.3.4"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run coverage"
        }
    },
    "keywords": [],
    "license": "MIT",
    "main": "bin/card.js",
    "name": "gligoran",
    "nyc": {
        "branches": 100,
        "check-coverage": true,
        "functions": 100,
        "include": [
            "bin/card.js"
        ],
        "lines": 100,
        "per-file": true,
        "statements": 100
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/gligoran/gligoran-card.git"
    },
    "scripts": {
        "clean": "rm -rf .nyc_output coverage dist",
        "coverage": "nyc npm test",
        "lint": "eslint bin",
        "lint:fix": "npm run lint -- --fix",
        "postcoverage": "nyc check-coverage",
        "report-coverage": "nyc report --reporter=lcov",
        "start": "node ./bin/card.js",
        "test": "npm run lint && ava",
        "test:watch": "npm test -- --watch",
        "upload-coverage": "codecov",
        "wipe": "npm run clean && rm -rf node_modules"
    },
    "version": "0.0.0"
}
